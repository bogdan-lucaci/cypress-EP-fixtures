                "knownIssue": "Notification delay more than 1 minute."
            },
            "actions": [
                {
                    "type": "request",
                    "body": "<?xml version=\"1.0\" encoding=\"utf-8\"?><Request version=\"1.0\" xmlns=\"http://europay.smart2pay.com/message\"><Action>OPEN_PREAPPROVAL</Action><MID>$MID</MID><MSID>1016</MSID><PaymentMethod>nuapay</PaymentMethod><IPAddress>127.0.0.1</IPAddress><Hash>$HASH</Hash><Details><Currency>EUR</Currency><ReturnURL>http://demo2.smart2pay.com/_redirectURLs/success.html</ReturnURL><Description>Preapproval Description</Description><Customer><Name>Test Buyer</Name><Email>buyer@smart2pay.com</Email><Address><CountryCode>DE</CountryCode><City>London</City><Line>Champs Elysee 345</Line></Address><Bank><IBAN>DE89370400440532013000</IBAN></Bank></Customer></Details></Request>",
                    "hashElements": [
                        "Action",
                        "MID",
                        "PaymentMethod",
                        "IPAddress",
                        "ReturnURL",
                        "Description"
                    ],
                    "expect": {
                        "StatusCode": 200,
                        "StatusID": "1"
                    }
                },
                {
                    "type": "visitEPLanding",
                    "preapprovalURL": "https://europaytest.smart2pay.com/Nuapay/Preapproval/Landing.aspx?ID="
                },
                {
                    "type": "pageInteraction",
                    "description": "Provider page 1",
                    "url": "sandbox.nuapay.com",
                    "elements": [
                        {
                            "path": "//input[@type='checkbox']",
                            "action": "check"
                        },
                        {
                            "path": "//a[@id='formbtn']",
                            "action": "click"
                        }
                    ]
                },
                {
                    "type": "pageInteraction",
                    "description": "Provider page 2",
                    "url": "demo2.smart2pay.com",
                    "elements": [
                        {
                            "path": "//div[@id='top_text'][contains(text(),'Transaction Successful!')]"
                        }
                    ]
                },
                {
                    "type": "dbQuery",
					"serverName": "APM_DB",
                    "delay": 65000,
                    "query": "SELECT ID, MerchantID, PaymentTypeID, InputDateTime, Description, PreapprovalStateDefID, NotificationDateTime, SubID, LastNotifiedPreapprovalStateDefID FROM PreapprovalRequests WHERE ID = @PREAPPROVALID",
                    "expect": {
                        "PreapprovalStateDefID": 2,
                        "PaymentTypeID": 101
                    }
                },
                {
                    "type": "request",
                    "body": "<?xml version=\"1.0\" encoding=\"utf-8\"?><Request version=\"1.0\" xmlns=\"http://europay.smart2pay.com/message\"><Action>INSERT_PREAPPROVED_PAYIN</Action><MID>$MID</MID><MSID>1016</MSID><PaymentMethod>nuapay</PaymentMethod><IPAddress>127.0.0.1</IPAddress><Hash>$HASH</Hash><Details><MTID>$MTID</MTID><Amount>100</Amount><Currency>EUR</Currency><PreapprovalID>$PREAPPROVALID</PreapprovalID><Description>Preapproved payment</Description><ExecutionDate>$TODAYDATE</ExecutionDate></Details></Request>",
                    "hashElements": [
                        "Action",
                        "MID",
                        "PaymentMethod",
                        "IPAddress",
                        "MTID",
                        "Amount",
                        "Currency",
                        "PreapprovalID"
                    ],
                    "expect": {
                        "StatusCode": 200,
                        "Amount": "100"
                    }
                },               
                {
                    "type": "dbQuery",
					"serverName": "APM_DB",
                    "delay": 2000,
                    "query": "SELECT [NuapayPayments].[Amount] as Amount , [NuapayPayments].[AvailableAmount] as AvailableAmount, [PaymentStateDefs].[Name] as Status FROM [Europay].[dbo].[NuapayPayments] with (NOLOCK), [PaymentStateDefs] where [NuapayPayments].[PaymentStateDefID] = [PaymentStateDefs].PaymentStateDefID and NuapayPayments.GPaymentID = @TRXID and [NuapayPayments].[MerchantID]=@MID",
                    "expect": {
                        "Status": "Open",
                        "Amount": 1.00,
                        "AvailableAmount": 1.00
                    }
                }
            ]
        },
